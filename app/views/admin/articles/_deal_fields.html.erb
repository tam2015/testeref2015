

<div class="field">

   <%= f.label :product_description %>
   <%= f.text_field :product_description, id: "product_description_auto_complete", data: {autocomplete_source: products_path}, class:"form-control"  %><br/><br/>

   <%= f.label :brand_name %>
   <%= f.text_field :brand_name, id: "brand_name_auto_complete", data: {autocomplete_source: brands_path}, class:"form-control"  %><br/><br/>

   <%= f.label :store_name %>
   <%= f.text_field :store_name, id: "store_name_auto_complete", data: {autocomplete_source: stores_path}, class:"form-control"  %><br/><br/>


   <%= f.label :category_name %>
   <%= f.collection_select :category_name, Admin::Category.all,:name,:name, include_blank: true  %><br /><br/>

 </div>





